{
    "name": "Questgem/telegram-bot",
    "description": "A simple Telegram bot using irazasyed/telegram-bot-sdk",
    "require": {
        "php": "^7.4 || ^8.0",
        "irazasyed/telegram-bot-sdk": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    },
    "minimum-stability": "stable"
}
